{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "background-color: var (-- color - primary)",
          "color: var (-- color - white)",
          "scrollbar-width"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/paint": "off",
    "axe/forms": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off"
  }
}